{
  "name": "wobc-backend",
  "version": "1.0.0",
  "description": "wobc-backend",
  "main": "index.js",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "knex": "^0.95.14",
    "knex-cleaner": "^1.3.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "eslint": "^8.4.1",
    "eslint-plugin-react": "^7.27.1",
    "nodemon": "^2.0.15"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "make": "knex migrate:make",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "restart": "npm run rollback && npm run migrate && npm run seed",
    "migrateh": "heroku run knex migrate:latest -a fitness-4-you",
    "rollbackh": "heroku run knex migrate:rollback -a fitness-4-you",
    "restarth": "npm run rollbackh && npm run migrateh && npm run seedh",
    "databaseh": "heroku pg:psql -a fitness-4-you",
    "seedh": "heroku run knex seed:run -a fitness-4-you",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
    "deploy": "git push heroku main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stu-wd/WOBC-backend.git"
  },
  "author": "stu-wd",
